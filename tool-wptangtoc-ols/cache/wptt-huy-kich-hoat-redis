#!/bin/bash
if [[ ! -f /etc/redis.conf ]];then
echo "Ban chua kich hoat redis"
exit
fi
. /etc/wptt/.wptt.conf
systemctl stop redis
systemctl disable redis
php_ver_chon=${php_version_check//[-._]/}
yum remove lsphp${php_ver_chon}-pecl-redis -y
rm -rf /etc/redis.conf
systemctl restart lsws
echo "Hoan tat huy kich hoat redis"
