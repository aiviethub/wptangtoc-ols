#!/bin/bash
if [[ ! -d /usr/local/lsmcd ]];then
echo "Ban chua kich hoat lsmemcached"
exit
fi
. /etc/wptt/.wptt.conf
php_ver_chon=${php_version_check//[-._]/}
yum remove lsphp${php_ver_chon}-pecl-memcached -y
systemctl stop lsmcd
systemctl disable lsmcd
rm -rf /usr/local/lsmcd
rm -rf ~/lsmcd
systemctl restart lsws
echo "Hoan tat tat LSmemcached"
