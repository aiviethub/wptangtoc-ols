#!/bin/bash
echo "========================================================================="
echo "|Quản lý tài nguyên => kiểm tra TOP	                                 |"
echo "========================================================================="
echo "Khi muốn thoát Ấn nút: q"
sleep 2
top
check_menu_wptangtoc_active=$1
if [[ $check_menu_wptangtoc_active = "98" ]];then
. /etc/wptt/wptt-tai-nguyen-main 1
fi
